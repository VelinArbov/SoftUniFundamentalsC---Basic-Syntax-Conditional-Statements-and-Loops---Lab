using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace tech
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int all = 0;
            for (int i = 1; i <= 10; i++)
            {
                all = n * i;
                Console.WriteLine($"{n} X {i} = {all}");
            }





        }

    }
}
